    },
    "require": {
        "ircmaxell/password-compat": "^1.0",
        "rakshazi/digestauth": "dev-master",
        "google/recaptcha": "~1.1",
        "zordius/lightncandy": "dev-master",
        "ksubileau/color-thief-php": "^1.3",
        "mailchimp/mailchimp": "^2.0",
        "cviebrock/discourse-php": "^0.9.1",
        "ezyang/htmlpurifier": "^4.9",
        "phpfastcache/phpfastcache": "^6.0",
        "cloudinary/cloudinary_php": "^1.8",
        "mobiledetect/mobiledetectlib": "^2.8",
        "pagerange/metaparsedown": "^1.0",
        "robquistnl/simplehtmltotext": "^1.1"
    }
