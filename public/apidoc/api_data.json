[  {    "type": "post",    "url": "/info/",    "title": "List available apps and their launch points",    "name": "Names",    "group": "App",    "version": "0.1.0",    "permission": [      {        "name": "bearer",        "title": "This method requires an Authoirization header to be present.",        "description": ""      }    ],    "description": "<p>List the app keys and their launch points that your subscription allows</p>",    "header": {      "fields": {        "Header": [          {            "group": "Header",            "type": "String",            "size": "32",            "optional": false,            "field": "Bearer",            "description": "<p>Your api-key</p>"          }        ]      },      "examples": [        {          "title": "Header-Example:",          "content": "{\n  \"Authorization\": \"Bearer: 1223b8c30a347321299611f873b449ad\"\n}",          "type": "json"        }      ]    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "Array",            "optional": false,            "field": "app",            "description": "<p>List of app properties</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "app.app_key",            "description": "<p>Internal identifier of app</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "app.launch",            "description": "<p>Launch url for app. You need to replace {token} with the value returned by the /createToken/ method</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "    HTTP/1.1 200 OK\n    [\n      \t{\n\t\t\"app_key\": \"docninja\",\n\t\t\"launch\": \"https://www.coursesuite.ninja/launch/docninja/{token}/\"\n\t}\n    }",          "type": "json"        }      ]    },    "filename": "./application/controller/ApiController.php",    "groupTitle": "App"  },  {    "type": "post",    "url": "/createToken/",    "title": "Generate an app access token using a licence key",    "name": "Generate",    "group": "Token",    "version": "0.1.0",    "permission": [      {        "name": "digest",        "title": "This method requires DIGEST authentication. Your apikey is the username, and secret_key is your password.",        "description": ""      }    ],    "description": "<p>In order to launch apps, you need to generate an access token. It validates your access using your api key and secret, and stores the publish-url (if set). When opening apps, this key will be validated for concurrency: you can only have as many apps running at the same time as you paid for.</p>",    "parameter": {      "fields": {        "Parameter": [          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "name",            "description": "<p>Your organisation name</p>"          },          {            "group": "Parameter",            "type": "String",            "size": "32",            "optional": false,            "field": "key",            "description": "<p>License key</p>"          },          {            "group": "Parameter",            "type": "String",            "optional": true,            "field": "publish_url",            "description": "<p>Url to enable publish-to feature (in supported apps)</p>"          }        ]      }    },    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "token",            "description": "<p>App access token</p>"          }        ]      },      "examples": [        {          "title": "Success-Response:",          "content": "HTTP/1.1 200 OK\n{\n  \"token\": \"$2$10,f8ds98f8f8023iy43y32i8f7ds89f79\"\n}",          "type": "json"        }      ]    },    "filename": "./application/controller/ApiController.php",    "groupTitle": "Token"  },  {    "success": {      "fields": {        "Success 200": [          {            "group": "Success 200",            "optional": false,            "field": "varname1",            "description": "<p>No type.</p>"          },          {            "group": "Success 200",            "type": "String",            "optional": false,            "field": "varname2",            "description": "<p>With type.</p>"          }        ]      }    },    "type": "",    "url": "",    "version": "0.0.0",    "filename": "./public/apidoc/main.js",    "group": "_Users_tim_Sites_CourseSuite_coursesuite_ninja_dev_public_apidoc_main_js",    "groupTitle": "_Users_tim_Sites_CourseSuite_coursesuite_ninja_dev_public_apidoc_main_js",    "name": ""  }]
